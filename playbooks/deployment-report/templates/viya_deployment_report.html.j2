<!doctype html>
<html lang="en">

<!-- ----------------------------------------------------------- -->
<!-- viya_deployment_report.html                                 -->
<!-- ----------------------------------------------------------- -->
<!-- Author: SAS Institute Inc.                                  -->
<!-- ----------------------------------------------------------- -->

<!-- BEGIN: head -->
<head>

<!-- meta -->
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">

<!-- page title -->
<title>Viya Deployment Report - {{ sas_deployment_details.created }}</title>
<link rel="icon" type="image/x-icon" href="data:image/x-icon;base64,">

<!-- jquery -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>
<script>
/* set up accordion class attributes */
$( function() {
    $( ".accordion" ).accordion({
        active: false,
        collapsible: true,
        heightStyle: "content",
        activate: function( event, ui ) {
            var scrollTop = $( ui.newHeader ).parent().scrollTop();
            if ( !ui.newHeader.length ) return;
            var top = $( ui.newHeader ).offset().top;
            $( "html,body" ).animate(
                {scrollTop: scrollTop + top - 128},
                "fast"
            );
        }
    });
    $( ".machines-accordion" ).accordion({
        icons: { "header": "ui-icon-circle-triangle-e", "activeHeader": "ui-icon-circle-triangle-s" }
    });
    $( ".machine-details-accordion" ).accordion({
        icons: { "header": "ui-icon-folder-collapsed", "activeHeader": "ui-icon-folder-open" }
    });
});
/* make links between nested accordions work like anchor links (maintains anchor link functionality) */
$( function() {
    $( ".nested-accordions-rel-link" ).click( function() {
        var target_header_id = $( this ).attr( 'href' ).substring( 1 );
        var curr_nested_accordion = $( '#' + $( this ).attr( 'accordion_id' ) );
        var target_nested_accordion = $( '#' + $( this ).attr( 'target_accordion_id' ) );
        var target_parent_header = target_nested_accordion.prev( '.ui-accordion-header' );
        var parent_accordion = target_parent_header.parent( '.accordion' );

        // close all current accordion panels
        curr_nested_accordion.accordion( 'option', 'active', false );

        // open target parent header
        var parent_headers = parent_accordion.find( 'h3.ui-accordion-header' );
        for (parent_header_index=0; parent_header_index < parent_headers.length; parent_header_index++) {
            if (parent_headers[parent_header_index].id == target_parent_header.attr( 'id' )) {
                parent_accordion.accordion( 'option', 'active', parent_header_index);
                break;
            }
        }

        // open target header
        var headers = target_nested_accordion.find( '.ui-accordion-header' );
        for (header_index=0; header_index < headers.length; header_index++) {
            if (headers[header_index].id == target_header_id) {
                target_nested_accordion.accordion( 'option', 'active', header_index);
                break;
            }
        }
    });
});
$( function() {
    var icon = $(".inline-icon");
    icon.each(function() {
        $(this).position({
            my: "right middle",
            at: "left middle",
            of: $(this).next()
        });
    });
});
</script>

<!-- style -->
<link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/themes/smoothness/jquery-ui.css">
<style>
body {
    font-family: Arial, Helvetica, sans-serif;
    min-width: 800px;
}

.fixed-title-bar {
    overflow: hidden;
    background-color: #005686;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 6em;
    color: white;
    display: flex;
    align-items: center;
    z-index: 100;
}

.fixed-title-bar img {
    height: 4em;
    padding: 1em;
}

.fixed-title-bar #radiance {
    position: absolute;
    right: -2em;
    height: 10em;
}

.fixed-title-bar #page-title-wrapper p {
    margin: 0;
}

.fixed-title-bar #page-title-wrapper #page-title {
    font-size: 2em;
    font-weight: bold;
}

.content {
    margin-top: 8em;
}

table {
    width: 75%;
    margin: 2em auto;
    border-collapse: collapse;
    font-size: 1em;
}

/* zebra striping */
tr:nth-of-type(odd) {
    background: #B7BBBF;
}

th {
    background: #005686;
    color: white;
    font-weight: bold;
}

td.package-update {
    background: #C0E3F6;
    color: black;
}

td, th {
    padding: 6px;
    border: 1px solid #ccc;
    text-align: left;
}

ul.striped-list {
    list-style-type: none;
    margin: 0;
    padding: 0;
}

ul.striped-list > li {
    padding: 0.25em;
}

ul.striped-list > li:nth-of-type(odd) {
    background-color: #B7BBBF;
}

ul.column-list {
    list-style-type: none;
    columns: 1;
    -webkit-columns: 1;
    -moz-columns: 1;
}

@media (min-width: 600px) {
    ul.column-list {
        columns: 2;
        -webkit-columns: 2;
        -moz-columns: 2;
    }
}

@media (min-width: 800px) {
    ul.column-list {
        columns: 3;
        -webkit-columns: 3;
        -moz-columns: 3;
    }
}

@media (min-width: 1000px) {
    ul.column-list {
        columns: 4;
        -webkit-columns: 4;
        -moz-columns: 4;
    }
}

.ui-accordion-header.service-down {
    background: #EC7063;
}

.ui-accordion-header.service-status-unknown {
    background: #ECB563;
}

.ui-accordion-header.package-update-available {
    background: #C0E3F6;
}
</style>
</head>
<!-- END: head -->


<!-- BEGIN: body -->
<body>

<!-- fixed title bar -->
<div class="fixed-title-bar">
    <img src="data:image/png;base64,"
         alt="SAS - The Power to Know">    <div id="page-title-wrapper">
        <p id="page-title">Viya Deployment Report</p>
        <p><sub>Created: {{ sas_deployment_details.created }}</sub></p>
    </div>
    <img id="radiance"
         src="data:image/png;base64,"
         alt="SAS radiance image">
</div>

<!-- content -->
<div class="content">

    <h1>Machines (<b>total</b>: {{ sas_deployment_details.sas_hosts | length }})</h1>

    <!-- machines accordion -->
    <div class="accordion machines-accordion">

{% for host in sas_deployment_details.sas_hosts | dictsort %}
{% if host[1]._unreachable %}
        <h2 id="machine-details-{{ host[1]._id }}-accordion-header">{{ host[0] }} (<font color="red">UNREACHABLE!</font>)</h2>
        <div class="machine-details">

            <!-- ansible host groups -->
            <div class="machine-details-host-groups">

                <h3>Inventory Host Groups</h3>
                <ul class="column-list">
{% for host_group in host[1].ansible_host_groups | sort %}
                    <li>{{ host_group }}</li>
{% endfor %}
                </ul>
            </div>
        </div>
{% else %}
        <h2 id="machine-details-{{ host[1]._id }}-accordion-header">{{ host[0] }} (<b>ipv4</b>: {{ host[1].ipv4 }})</h2>
        <div class="machine-details">

            <!-- os details -->
            <div class="machine-details-os">

                <h3>Operating System</h3>

                <table>
                    <tr>
                        <th>Family</th>
                        <td>{{ host[1].os.family }}</td>
                    </tr>
                    <tr>
                        <th>Distribution</th>
                        <td>{{ host[1].os.distribution }}</td>
                    </tr>
                    <tr>
                        <th>Version</th>
                        <td>{{ host[1].os.version }}</td>
                    </tr>
                    <tr>
                        <th>Architecture</th>
                        <td>{{ host[1].os.architecture }}</td>
                    </tr>
                    <tr>
                        <th>Package Manager</th>
                        <td>{{ host[1].os.package_manager }}</td>
                    </tr>
                </table>
            </div>

            <!-- ansible host groups -->
            <div class="machine-details-host-groups">

                <h3>Installed Host Groups</h3>
                <ul class="column-list">
{% for host_group in host[1].ansible_host_groups | sort %}
                    <li>{{ host_group }}</li>
{% endfor %}
                </ul>
            </div>

            <!-- machine details accordion -->
            <div class="accordion machine-details-accordion" id="{{ host[1]._id }}-machine-details-accordion">

                <! -- BEGIN: resources -->

                <h3 id="{{ host[1]._id }}-machine-details-accordion-resources-header">Resources</h3>
                <div>

                    <!-- memory -->
                    <h4 id="{{ host[1]._id }}-resources-memory-header">Memory</h4>
                    <table>
                        <tr>
                            <th></th>
                            <th>Total</th>
                            <th>Used</th>
                            <th>Free</th>
                            <th>Shared</th>
{% if host[1].resource_check.memory.results_format == 'current' %}
                            <th>Buff/Cache</th>
                            <th>Available</th>
{% else %}
                            <th>Buffers</th>
                            <th>Cached</th>
{% endif %}
                        </tr>
{% for memory in host[1].resource_check.memory.results | dictsort %}
                        <tr>
                            <th>{{ memory[0] }}</th>
                            <td>{{ memory[1].total }}</td>
                            <td>{{ memory[1].used }}</td>
                            <td>{{ memory[1].free }}</td>
                            <td>{{ memory[1].shared }}</td>
{% if host[1].resource_check.memory.results_format == 'current' %}
                            <td>{{ memory[1].buff_cache }}</td>
                            <td>{{ memory[1].available }}</td>
{% else %}
                            <td>{{ memory[1].buffers }}</td>
                            <td>{{ memory[1].cached }}</td>
{% endif %}
                        </tr>
{% endfor %}
                        <caption align="bottom"><sub>Results as of: {{ host[1].resource_check.memory.results_timestamp }}</sub></caption>
                    </table>

                    <!-- SAS root -->
                    <h4 id="{{ host[1]._id}}-resources-sas-root-header">SAS Installation Root</h4>
                    <table>
                        <tr>
                            <th>Path</th>
                            <th>Size</th>
                            <th>Mount</th>
                            <th>Filesystem</th>
                            <th>Filesystem Total Size</th>
                            <th>Used %</th>
                        </tr>
                        <tr>
                            <td>{{ host[1].resource_check.sas_root.results.path }}</td>
                            <td>{{ host[1].resource_check.sas_root.results.size }}</td>
                            <td>{{ host[1].resource_check.sas_root.results.mount }}</td>
                            <td>{{ host[1].resource_check.sas_root.results.filesystem }}</td>
                            <td>{{ host[1].resource_check.sas_root.results.filesystem_total }}</td>
                            <td>{{ host[1].resource_check.sas_root.results.used_ratio }}</td>
                        </tr>

                        <caption align="bottom"><sub>Results as of: {{ host[1].resource_check.sas_root.results_timestamp }}</sub></caption>
                    </table>

                    <!-- filesystems -->
                    <h4 id="{{ host[1]._id }}-resources-filesystems-header">Filesystems</h4>
                    <table>
                        <tr>
                            <th>Filesystem</th>
                            <th>Type</th>
                            <th>Size</th>
                            <th>Used</th>
                            <th>Available</th>
                            <th>Used %</th>
                            <th>Mounted On</th>
                        </tr>
{% for filesystem in host[1].resource_check.filesystems.results | dictsort %}
                        <tr>
                            <td>{{ filesystem[1].filesystem }}</td>
                            <td>{{ filesystem[1].type }}</td>
                            <td>{{ filesystem[1].size }}</td>
                            <td>{{ filesystem[1].used }}</td>
                            <td>{{ filesystem[1].available }}</td>
                            <td>{{ filesystem[1].used_ratio }}</td>
                            <td>{{ filesystem[1].mounted_on }}</td>
                        </tr>
{% endfor %}
                        <caption align="bottom"><sub>Results as of: {{ host[1].resource_check.filesystems.results_timestamp }}</sub></caption>
                    </table>
                </div>

                <!-- END: resources -->

                <!-- BEGIN: packages -->

                <h3 id="{{ host[1]._id }}-machine-details-accordion-packages-header">Packages (<b>total</b>: {{ host[1].sas_packages | length }}{% if host[1].available_package_updates != 0 %} | {{ host[1].available_package_updates }} updates available <span class="ui-icon ui-icon-circle-arrow-n inline-icon"></span>{% endif %})</h3>

                <!-- packages accordion -->
                <div class="accordion packages-accordion" id="{{ host[1]._id }}-packages-accordion">

{% for package in host[1].sas_packages | dictsort %}
                    <h4 id="{{ host[1]._id }}-package-{{ package[0] }}-accordion-header" {% if package[1].update_status.available %}class="package-update-available"{% endif %}>{{ package[0] }}-{{ package[1].attributes.version }}{% if package[1].update_status.available %} <span class="ui-icon ui-icon-circle-arrow-n inline-icon"></span>{% endif %}</h4>
                    <div>
                        <table>
                            <caption>Details</caption>
{% for package_attr in package[1].attributes | dictsort %}
                            <tr>
                                <th>{{ package_attr[0] | replace('_', ' ') | title }}</th>
                                <td>{{ package_attr[1] }}</td>
                            </tr>
{% endfor %}
                            <tr>
                                <th>Update Status</th>
{% if package[1].update_status.available %}
                                <td class="package-update">{{ package[1].update_status.version }} (available)</td>
{% else %}
                                <td>up-to-date</td>
{% endif %}
                            </tr>
{% if package[1].provided_services | length > 0 %}
                            <tr>
                                <th>Provided Services</th>
                                <td>
                                    <ul>
{% for provided_service in package[1].provided_services | sort %}
                                        <li>
                                            <a class="nested-accordions-rel-link"
                                               href="#{{ host[1]._id }}-service-{{ provided_service }}-accordion-header"
                                               accordion_id="{{ host[1]._id }}-packages-accordion"
                                               target_accordion_id="{{ host[1]._id }}-services-accordion">{{ provided_service }}</a>
                                        </li>
{% endfor %}
                                    </ul>
                                </td>
                            </tr>
{% endif %}
                        </table>

{% if package[1].installed_files | length > 0 %}
                        <div id="{{ host[1]._id }}-package-{{ package[0] }}-installed-files-accordion" class="accordion package-installed-files-accordion">
                            <h3 id="{{ host[1]._id }}-package-{{ package[0] }}-installed-files-accordion-header">Installed files</h3>

                            <div>
                                <ul class="striped-list">
{% for file in package[1].installed_files | sort %}
                                    <li>{{ file }}</li>
{% endfor %}
                                </ul>
                            </div>
                        </div>
{% endif %}
                    </div>
{% endfor %}
                </div>

                <!-- END: packages -->

                <!-- BEGIN: services -->

                <h3 id="{{ host[1]._id }}-machine-details-accordion-services-header">Services &nbsp;(<b>total</b>: {{ host[1].sas_services.installed | length }} | <b>up</b>: {{ host[1].sas_services.status.up }} | <b>down</b>: {{ host[1].sas_services.status.down }} | <b>other</b>: {{ host[1].sas_services.status.other }} | <b>memory</b>: {{ host[1].sas_services.status.memory }})</h3>

                <!-- services accordion -->
                <div class="accordion services-accordion" id="{{ host[1]._id }}-services-accordion">

{% for service in host[1].sas_services.installed | dictsort %}
                    <h4 id="{{ host[1]._id }}-service-{{ service[0] }}-accordion-header" class="{% if service[1].attributes.status == 'up' or service[1].attributes.status == 'down' %}service-{{ service[1].attributes.status }}{% else %}service-status-unknown{% endif %}">{{ service[0] }} (<b>status</b>: {{ service[1].attributes.status }})</h4>
                    <div>
                        <table>
                            <caption>Details</caption>
{% for service_attr in service[1].attributes | dictsort %}
                            <tr>
                                <th>{{ service_attr[0] | replace('_', ' ') | title }}</th>
                                <td>{{ service_attr[1] }}</td>
                            </tr>
{% endfor %}
{% if service[1].installed_by is defined and service[1].installed_by != "" %}
                            <tr>
                                <th>Installed By</th>
                                <td>
                                    <a class="nested-accordions-rel-link"
                                       href="#{{ host[1]._id }}-package-{{ service[1].installed_by }}-accordion-header"
                                       accordion_id="{{ host[1]._id }}-services-accordion"
                                       target_accordion_id="{{ host[1]._id }}-packages-accordion">{{ service[1].installed_by }}</a>
                                </td>
                            </tr>
{% endif %}
                        </table>
                    </div>
{% endfor %}
                </div>

                <!-- END: services -->

            </div>
        </div>
{% endif %}
{% endfor %}
    </div>
</div>

</body>
</html>
